<%= form_for(@user_session) do |f| %>
  <%= render :partial => "shared/user_error", :locals => {:model_object => @user_session} %>

    <div class="field">
      <%= f.label :username %>
      <br/>
      <%= f.text_field :username %>
    </div>
    <div class="field">
      <%= f.label :password %>
      <br/>
      <%= f.password_field :password %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
