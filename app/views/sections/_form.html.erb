<%= form_for(@section) do |f| %>
  <%= render :partial => "shared/user_error", :locals => {:model_object => @section} %>

  <div class="field">
    <%= f.label :year %><br />
    <%= f.text_field :year %>
  </div>
  <div class="field">
    <%= f.label :semester %><br />
    <%= f.number_field :semester %>
  </div>
  <div class="field">
    <%= f.label :section %><br />
    <%= f.number_field :section %>
  </div>
  <div class="field">
    <%= f.label :is_suspend %><br />
    <%= f.check_box :is_suspend %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
